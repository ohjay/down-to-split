<%= simple_form_for(@shopping_trip) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.association :vendor %>

    <%= f.association :user %>

    <%= f.input :total %>

    <%= f.input :date %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
